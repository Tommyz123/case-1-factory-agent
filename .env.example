# Azure OpenAI Configuration (Required)
AZURE_OPENAI_ENDPOINT=<your-azure-endpoint>
AZURE_OPENAI_API_KEY=<your-azure-api-key>
AZURE_OPENAI_API_VERSION=2024-12-01-preview
AZURE_OPENAI_DEPLOYMENT_NAME=<your-gpt-deployment-name>
AZURE_OPENAI_EMBEDDING_DEPLOYMENT=<your-embedding-deployment-name>
AZURE_OPENAI_EMBEDDING_API_VERSION=2024-02-01

# Search API Configuration
SEARCH_METHOD=web_scraping

# Optional: Tavily Search API (if you have the key)
# SEARCH_METHOD=tavily
# TAVILY_API_KEY=<your-tavily-key>

# Optional: SerpAPI (if you have the key)
# SEARCH_METHOD=serpapi
# SERPAPI_API_KEY=<your-serpapi-key>

# Logging
LOG_LEVEL=INFO
